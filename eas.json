{
	"cli": {
		"version": ">= 16.19.2",
		"appVersionSource": "remote"
	},
	"build": {
		"development": {
			"developmentClient": true,
			"distribution": "internal",
			"channel": "development",
			"env": {
				"APP_VARIANT": "development",
				"RELEASE_CHANNEL": "development"
			}
		},
		"preview": {
			"distribution": "store",
			"channel": "preview",
			"extends": "production",
			"env": {
				"APP_VARIANT": "preview",
				"RELEASE_CHANNEL": "preview"
			}
		},
		"production": {
			"autoIncrement": true,
			"channel": "production",
			"credentialsSource": "remote"
		}
	},
	"submit": {
		"production": {
			"android": {
				"track": "production",
				"changesNotSentForReview": true
			},
			"ios": {
				"appleId": "douglasndm@outlook.com",
				"ascAppId": "1549418617",
				"appleTeamId": "F3ZQ4BPF29"
			}
		},
		"preview": {
			"extends": "production",
			"android": {
				"track": "beta",
				"changesNotSentForReview": false
			},
			"ios": {
				"appleId": "douglasndm@outlook.com",
				"ascAppId": "1549418617",
				"appleTeamId": "F3ZQ4BPF29"
			}
		},
		"development": {
			"android": {
				"track": "internal",
				"changesNotSentForReview": false
			}
		}
	}
}
